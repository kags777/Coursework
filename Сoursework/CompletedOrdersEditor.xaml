<UserControl x:Class="Coursework.CompletedOrdersEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Margin="10">
        <TextBlock Text="Выполненные заказы" FontWeight="Bold"/>

        <ListBox x:Name="OrdersList"
                 DisplayMemberPath="DisplayName"
                 Height="300"/>

        <Button Content="Просмотр"
                Margin="0,10,0,0"
                Click="View_Click"/>
    </StackPanel>
</UserControl>
